{
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed",
    "overrides": [
        {
            "files": "*.css",
            "options": {
                "parser": "css"
            }
        },
        {
            "files": "*.scss",
            "options": {
                "parser": "scss"
            }
        }
    ]
}